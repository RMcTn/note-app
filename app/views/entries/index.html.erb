<%= link_to 'Create new entry', new_entry_path %>
<table>
<%= @entries.each do |entry| %>
  <tr>
  <td><%= entry.title %></td>
  <td><%= entry.text %></td>
  <td><%= entry.content %></td>
  <td><%= link_to 'Show', entry_path(entry) %></td>
  <td><%= link_to 'Edit', edit_entry_path(entry) %></td>
  <td><%= link_to 'Delete', entry_path(entry),
    method: :delete,
    data: {confirm: 'Are you sure?'} %></td>
  </tr>
<% end %>
</table>
